{#############################################

Purpose:
Configure ntp commands for dellos10 Devices

###############################################}

!
ntp authenticate
{% for server in ntp.server %}
{% if server.prefer %}
ntp server {{ server.addr }} prefer
{% endif %}
ntp server {{ server.addr }}
{% endfor %}
{% if ntp.src_intf %}
ntp source {{ ntp.src_intf }}
{% endif %}
